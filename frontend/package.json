{
  "name": "Lark-UI",
  "version": "1.0.0",
  "description": "Lark is the microblogging platform powered by mina blockchain",
  "repository": "https://github.com/wotori/Lark",
  "homepage": "https://wotori.github.io/Lark/",
  "author": "Wotori Movako",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "mkdir dist && cp -r ./public/* ./dist/ && yarn parcel src/index.html",
    "build": "yarn parcel build ./src/index.html --dist-dir build --no-source-maps && cp -r ./public/* ./build/",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "parcel": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.20",
    "process": "^0.11.10",
    "tailwindcss": "^3.2.4"
  }
}
